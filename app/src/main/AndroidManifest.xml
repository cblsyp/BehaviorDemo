<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.administrator.behaviordemo">
    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW"/>
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:name=".MyApplications"
        android:theme="@style/MDTheme">
        <activity android:name=".activity.JanshuActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity"/>
        <activity android:name=".activity.EasyBehaviorActivity"/>
        <activity android:name=".activity.ToolBarActivity"/>
        <activity android:name=".activity.WindowActivity"/>
        <activity android:name=".activity.StausBarBiewActivity"/>
        <activity android:name=".activity.StausBarBiewActivity2"/>
        <activity android:name=".activity.AppbarLayoutActivity"/>
    </application>

</manifest>